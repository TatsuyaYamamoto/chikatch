{
  "name": "chikatch-console",
  "version": "0.1.0",
  "description": "Desktop Console App for Chikatch, Wireless Psyllium Sysytem.",
  "main": "app/src/main/index.js",
  "scripts": {
    "start": "electron .",
    "install": "npm run rebuild",
    "rebuild": "electron-rebuild",
    "pack": "run-s pack:*",
    "pack:mac": "rm -rf dist/mac/ dist/*.dmg* dist/latest-mac.yml && build --mac",
    "pack:win": "rm -rf dist/win-unpacked/ dist/*.exe dist/latest.yml && build --win",
    "archive": "run-s archive:*",
    "archive:osx": "npm run pack:mac && zip -j dist/ChikatchConsole.dmg.zip dist/*.dmg",
    "archive:win": "npm run pack:win && zip -j dist/ChikatchConsole.exe.zip dist/*.exe"
  },
  "build": {
    "productName": "Chikatch Console",
    "appId": "net.sokontokoro-factory.apps.chikatch",
    "mac": {
      "target": "dmg"
    },
    "win": {
      "target": "nsis"
    }
  },
  "repository": "https://github.com/shop021/chikatch.git",
  "author": "Tatsuya Yamamoto",
  "license": "MIT",
  "devDependencies": {
    "electron": "~1.7.8",
    "electron-builder": "^19.45.5",
    "electron-rebuild": "^1.6.0",
    "npm-run-all": "^4.1.2"
  },
  "dependencies": {
    "material-design-icons": "^3.0.1",
    "materialize-css": "^0.100.2",
    "twelite-sdk": "^0.0.2",
    "vue": "^2.5.7"
  }
}
