{
  "name": "chikatch-console",
  "version": "0.0.2",
  "description": "Desktop Console App for Chikatch, Wireless Psyllium Sysytem.",
  "main": "app/src/main.js",
  "scripts": {
    "start": "electron .",
    "install": "npm run rebuild",
    "rebuild": "electron-rebuild",
    "pack:mac": "build --mac",
    "pack:win": "build --win"
  },
  "build": {
    "productName": "Chikatch Console",
    "appId": "net.sokontokoro-factory.apps.chikatch",
    "mac": {
      "target": "dmg"
    },
    "win": {
      "target": "nsis"
    }
  },
  "repository": "https://github.com/chikatch/chikatch-console.git",
  "author": "Tatsuya Yamamoto",
  "license": "MIT",
  "devDependencies": {
    "electron": "~1.7.8",
    "electron-builder": "^19.45.5",
    "electron-rebuild": "^1.6.0"
  },
  "dependencies": {
    "material-design-icons": "^3.0.1",
    "materialize-css": "^0.100.2",
    "twelite-sdk": "^0.0.2",
    "vue": "^2.5.7"
  }
}
